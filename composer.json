{
    "name": "decodelabs/phpstan-decodelabs",
    "description": "DecodeLabs PHPStan compatibility extension",
    "type": "phpstan-extension",
    "keywords": ["analysis"],
    "license": "MIT",
    "authors": [{
        "name": "Tom Wright",
        "email": "tom@inflatablecookie.com"
    }],
    "require": {
        "php": "^8.0",
        "phpstan/phpstan": "^1"
    },
    "require-dev": {
        "decodelabs/tagged": "^0.14",
        "decodelabs/exemplar": "^0.3",
        "decodelabs/lucid": "^0.3.5",
        "decodelabs/metamorph": "^0.5",
        "decodelabs/terminus": "^0.9"
    },
    "autoload": {
        "psr-4": {
            "DecodeLabs\\PHPStan\\": "src/"
        }
    },
    "extra": {
        "phpstan": {
            "includes": [
                "extension.neon"
            ]
        },
        "branch-alias": {
            "dev-develop": "0.6.x-dev"
        }
    }
}
