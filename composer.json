{
    "name": "decodelabs/phpstan-decodelabs",
    "description": "DecodeLabs PHPStan analysis extension",
    "type": "phpstan-extension",
    "keywords": [ "dev", "static-analysis" ],
    "license": "MIT",
    "authors": [ {
            "name": "Tom Wright",
            "email": "tom@inflatablecookie.com"
        } ],
    "require": {
        "php": "^8.4",
        "phpstan/phpstan": "^2",
        "phpstan/extension-installer": "^1.4"
    },
    "autoload": {
        "psr-4": {
            "DecodeLabs\\PHPStan\\": "src/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-develop": "0.7.x-dev"
        },
        "effigy": {
            "exports": [
                "phpstan-extension.neon"
            ]
        }
    },
    "config": {
        "allow-plugins": {
            "phpstan/extension-installer": true
        }
    }
}
