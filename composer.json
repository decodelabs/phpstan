{
    "name": "decodelabs/phpstan-decodelabs",
    "description": "DecodeLabs PHPStan compatibility extension",
    "type": "phpstan-extension",
    "keywords": [ "analysis" ],
    "license": "MIT",
    "authors": [ {
            "name": "Tom Wright",
            "email": "tom@inflatablecookie.com"
        } ],
    "require": {
        "php": "^8.1",
        "phpstan/phpstan": "^1",
        "phpstan/extension-installer": "^1.4"
    },
    "require-dev": {
        "decodelabs/collections": "^0.9",
        "decodelabs/exceptional": "^0.4.7",
        "decodelabs/exemplar": "^0.4",
        "decodelabs/lucid": "^0.4.7",
        "decodelabs/metamorph": "^0.6",
        "decodelabs/tagged": "^0.15",
        "decodelabs/terminus": "^0.10",
        "decodelabs/veneer": "^0.11.6"
    },
    "conflict": {
        "decodelabs/veneer": "<0.11.6"
    },
    "autoload": {
        "psr-4": {
            "DecodeLabs\\PHPStan\\": "src/"
        }
    },
    "extra": {
        "effigy": {
            "exports": [
                "phpstan-extension.neon"
            ]
        },
        "phpstan": {
            "includes": [
                "phpstan-extension.neon"
            ]
        },
        "branch-alias": {
            "dev-develop": "0.6.x-dev"
        }
    },
    "config": {
        "allow-plugins": {
            "phpstan/extension-installer": true
        }
    }
}
